@model ranglerz_project.ModelsBO.RestrictUser

@{
    ViewBag.Title = "All Users";
}




<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>User view</title>

    <!-- Bootstrap core CSS -->

    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">

    <link href="~/fonts/font-awesome.min.css" rel="stylesheet">
    <link href="~/Content/css/animate.min.css" rel="stylesheet">

    <!-- Custom styling plus plugins -->
    <link href="~/Content/css/custom.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/Content/css/jquery-jvectormap-2.0.1.css" />
    <link href="~/Content/css/green.css" rel="stylesheet" />
    <link href="~/Content/css/floatexamples.css" rel="stylesheet" type="text/css" />

    <script src="~/Scripts/js/jquery.min.js"></script>
 

</head>




<body class="nav-md">

    <div class="container body">


        <div class="main_container">



            @{
                Html.RenderPartial("_Partial");
            }




        
            <div class="right_col" role="main">
                <div class="">
                    <div class="page-title">
                        <div class="title_left">
                            <h3>
                               User View
                            </h3>
                        </div>

                        
                    </div>
                    <div class="clearfix"></div>

                    <div class="row">

                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <div class="x_panel">
                                <div class="x_title">
                                    <h2> User View List</h2>
                                   
                                    <div class="clearfix"></div>
                                </div>
                                <div class="x_content">
                                    <table id="example" class="table table-striped responsive-utilities jambo_table">
                                        <thead>
                                            <tr class="headings">
                                                <th>
                                                    <input type="checkbox" class="tableflat">
                                                </th>
                                                <th>Name </th>
                                                <th>Username </th>
                                                <th>Password </th>
                                                <th>Creation time </th>
                                                <th>Status </th>
                                                @if(@Model.edit=="Y")
                                                { 
                                                <th>Action </th>
                                                }
                                                @if(@Model.delete=="Y")
                                                { 
                                                <th class=" no-link last">
                                                    <span class="nobr">Action</span>
                                                </th>
                                                }
                                            </tr>
                                        </thead>

                                        <tbody>
                                           

                                                @foreach (var v in @Model.allusers)
                                            {
                                                <tr class="even pointer">
                                                    <td class="a-center ">
                                                        <input type="checkbox" class="tableflat">
                                                    </td>
                                                    <td class=" ">@v.name</td>
                                                    <td class=" ">@v.username </td>
                                                    <td class=" ">
                                                        @v.password
                                                    </td>
                                                    <td class=" ">@v.created_at</td>
                                                    @if(@v.is_active=="Y")
                                                    { 
                                                    <td class=" ">Active</td>
                                                    }
                                                    @if (@v.is_active == "N")
                                                    {
                                                        <td class=" ">Deactive</td>
                                                    }
                                                    @if (@Model.edit=="Y")
                                                    {
                                                    <td>
                                                        <a href="~/USER_CRUD/Edit/@v.Id/">Update</a>
                                                    </td>
                                                    }
                                              
                                                    @if (@Model.delete == "Y")
                                                    { 
                                                    <td class=" last">
                                                        <a href="~/USER_CRUD/Delete/@v.Id/">Delete</a>
                                                    </td>
                                                    }
                                                </tr>
                                            }

                                            
                                        </tbody>

                                    </table>
                                </div>
                            </div>
                        </div>

                        <br />
                        <br />
                        <br />

                    </div>
                </div>
           
            </div>
            <!-- /page content -->
        </div>

    </div>

    <script src="~/Scripts/js/bootstrap.min.js"></script>

    <script src="~/Scripts/js/custom.js"></script>
    
</body>

</html>